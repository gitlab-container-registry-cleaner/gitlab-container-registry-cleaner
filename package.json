{
  "name": "@gitlab-container-registry-cleaner/gitlab-container-registry-cleaner",
  "description": "A CLI tool to clean up the GitLab Container Registry",
  "author": "gitlab-container-registry-cleaner",
  "license": "Apache-2.0",
  "homepage": "https://github.com/gitlab-container-registry-cleaner/gitlab-container-registry-cleaner",
  "repository": "https://github.com/gitlab-container-registry-cleaner/gitlab-container-registry-cleaner",
  "version": "0.1.0",
  "type": "module",
  "types": "./dist/main.d.ts",
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@mswjs/interceptors": "^0.39.7",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.6.1",
    "auto-changelog": "^2.5.0",
    "msw": "^2.11.3",
    "np": "^10.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.20.0"
  },
  "bin": "./gitlab-container-registry-cleaner",
  "exports": {
    ".": "./dist/main.js"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE.txt",
    "README.md",
    "gitlab-container-registry-cleaner"
  ],
  "dependencies": {
    "@gitbeaker/cli": "^43.5.0",
    "@gitbeaker/rest": "^43.5.0",
    "commander": "^14.0.1",
    "dotenv": "^17.2.3",
    "inquirer": "^12.9.6"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "commitLimit": false
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "test": "vitest",
    "dev": "tsx src/main.ts",
    "version": "npx auto-changelog -p && git add CHANGELOG.md",
    "release": "yarn build && np",
    "lint": "biome check .",
    "lint:fix": "biome check . --write"
  }
}
